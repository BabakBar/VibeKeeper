{
  "app_lifecycle": {
    "startup": [
      {
        "file": "VibeKeeper/VibeKeeperApp.swift",
        "entity": "VibeKeeperApp",
        "description": "SwiftUI App entry point",
        "functionality": "Initializes the application and sets up the main view hierarchy"
      }
    ],
    "content_rendering": [
      {
        "file": "VibeKeeper/ContentView.swift",
        "entity": "ContentView",
        "description": "Main content view",
        "functionality": "Renders the primary UI for the application"
      }
    ]
  },
  "planned_structures": {
    "data_models": [
      {
        "entity": "User",
        "description": "User model",
        "properties": ["id", "email", "name", "preferences", "created_at", "updated_at"],
        "relationships": [
          {"name": "contacts", "type": "one-to-many", "target": "Contact"},
          {"name": "gift_ideas", "type": "one-to-many", "target": "GiftIdea"},
          {"name": "reminders", "type": "one-to-many", "target": "Reminder"}
        ]
      },
      {
        "entity": "Contact",
        "description": "Contact/recipient model",
        "properties": ["id", "user_id", "name", "relationship", "preferences", "notes", "created_at", "updated_at"],
        "relationships": [
          {"name": "user", "type": "many-to-one", "target": "User"},
          {"name": "gift_ideas", "type": "one-to-many", "target": "GiftIdea"},
          {"name": "occasions", "type": "one-to-many", "target": "Occasion"}
        ]
      },
      {
        "entity": "GiftIdea",
        "description": "Gift idea model",
        "properties": ["id", "user_id", "recipient_id", "idea", "occasion", "date", "tags", "budget_amount", "budget_currency", "budget_range_min", "budget_range_max", "status", "notes", "created_at", "updated_at"],
        "relationships": [
          {"name": "user", "type": "many-to-one", "target": "User"},
          {"name": "recipient", "type": "many-to-one", "target": "Contact"},
          {"name": "media_assets", "type": "one-to-many", "target": "MediaAsset"},
          {"name": "reminders", "type": "one-to-many", "target": "Reminder"}
        ]
      },
      {
        "entity": "Reminder",
        "description": "Reminder model",
        "properties": ["id", "user_id", "gift_idea_id", "scheduled_for", "message", "channel", "status", "created_at", "updated_at"],
        "relationships": [
          {"name": "user", "type": "many-to-one", "target": "User"},
          {"name": "gift_idea", "type": "many-to-one", "target": "GiftIdea"}
        ]
      },
      {
        "entity": "Occasion",
        "description": "Occasion model",
        "properties": ["id", "contact_id", "name", "date", "recurring", "created_at", "updated_at"],
        "relationships": [
          {"name": "contact", "type": "many-to-one", "target": "Contact"}
        ]
      },
      {
        "entity": "MediaAsset",
        "description": "Media asset model",
        "properties": ["id", "gift_idea_id", "url", "type", "created_at"],
        "relationships": [
          {"name": "gift_idea", "type": "many-to-one", "target": "GiftIdea"}
        ]
      }
    ],
    "view_models": [
      {
        "entity": "AuthViewModel",
        "description": "Authentication view model",
        "methods": ["login", "register", "logout", "refreshToken", "getCurrentUser"]
      },
      {
        "entity": "GiftIdeaViewModel",
        "description": "Gift idea view model",
        "methods": ["getGiftIdeas", "getGiftIdea", "createGiftIdea", "updateGiftIdea", "deleteGiftIdea"]
      },
      {
        "entity": "ContactViewModel",
        "description": "Contact view model",
        "methods": ["getContacts", "getContact", "createContact", "updateContact", "deleteContact"]
      },
      {
        "entity": "ReminderViewModel",
        "description": "Reminder view model",
        "methods": ["getReminders", "getReminder", "createReminder", "updateReminder", "deleteReminder"]
      },
      {
        "entity": "CaptureViewModel",
        "description": "Capture view model",
        "methods": ["captureText", "startVoiceRecording", "stopVoiceRecording", "captureImage", "processImageCapture"]
      },
      {
        "entity": "SearchViewModel",
        "description": "Search view model",
        "methods": ["search", "getSearchSuggestions", "clearSearchHistory"]
      }
    ],
    "views": [
      {
        "entity": "LoginView",
        "description": "Login view",
        "view_model": "AuthViewModel"
      },
      {
        "entity": "RegisterView",
        "description": "Register view",
        "view_model": "AuthViewModel"
      },
      {
        "entity": "GiftIdeaListView",
        "description": "Gift idea list view",
        "view_model": "GiftIdeaViewModel"
      },
      {
        "entity": "GiftIdeaDetailView",
        "description": "Gift idea detail view",
        "view_model": "GiftIdeaViewModel"
      },
      {
        "entity": "GiftIdeaEditView",
        "description": "Gift idea edit view",
        "view_model": "GiftIdeaViewModel"
      },
      {
        "entity": "ContactListView",
        "description": "Contact list view",
        "view_model": "ContactViewModel"
      },
      {
        "entity": "ContactDetailView",
        "description": "Contact detail view",
        "view_model": "ContactViewModel"
      },
      {
        "entity": "ContactEditView",
        "description": "Contact edit view",
        "view_model": "ContactViewModel"
      },
      {
        "entity": "CaptureView",
        "description": "Capture view",
        "view_model": "CaptureViewModel"
      },
      {
        "entity": "TextCaptureView",
        "description": "Text capture view",
        "view_model": "CaptureViewModel"
      },
      {
        "entity": "VoiceCaptureView",
        "description": "Voice capture view",
        "view_model": "CaptureViewModel"
      },
      {
        "entity": "ImageCaptureView",
        "description": "Image capture view",
        "view_model": "CaptureViewModel"
      },
      {
        "entity": "SearchView",
        "description": "Search view",
        "view_model": "SearchViewModel"
      },
      {
        "entity": "ReminderListView",
        "description": "Reminder list view",
        "view_model": "ReminderViewModel"
      },
      {
        "entity": "ReminderDetailView",
        "description": "Reminder detail view",
        "view_model": "ReminderViewModel"
      }
    ]
  }
}